{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-unused-vars": "warn",
    "camelcase": "off",
    "no-param-reassign": "warn",
    "indent": "off",
    "max-classes-per-file": "off",
    "no-useless-constructor": "off",
    "object-curly-newline": "off"
  }
}
